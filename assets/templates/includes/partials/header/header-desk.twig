<!-- begin header-desk -->
<div class="header-desk uk-visible@l">
	<div class="header-desk__top">
		<div class="uk-container">
			<div class="header-desk__inner">
				<div class="uk-grid">
					<div class="uk-width-auto">
						{% include "../logo.twig" %}
					</div>
					<div class="uk-width-expand">
						<div class="uk-flex uk-flex-middle uk-flex-between">
							<div class="search-desk">
								<form action="" class="search-form">
									<input type="text" placeholder="Поиск товара">
									<button>
										<svg width="24px" height="24px" class="icon-search">
											<use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="img/icon-svg/sprite/sprite.svg#search"></use>
										</svg>
									</button>
								</form>
							</div>

							<div class="contacts uk-flex">
								<div class="contacts__item">
									<a href="tel:+79267868151" class="contacts__link">
										<span>+7 (926) 786 81 51</span>
										<span class="icons">
											<svg width="22px" height="22px" class="icon-whataspp">
												<use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="img/icon-svg/sprite/sprite.svg#whatsapp"></use>
											</svg>
											<svg width="22px" height="22px" class="icon-viber">
												<use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="img/icon-svg/sprite/sprite.svg#viber"></use>
											</svg>
										</span>
									</a>
									<div class="contacts__text">
										Ежедневно с 09.00 до 18.00
									</div>
								</div>
								<div class="contacts__item">
									<a href="tel:+74955323596" class="contacts__link">
										<span>8 (495) 532 35 96</span>
									</a>
									<div class="contacts__text">
										<a href="#callback" uk-toggle>Заказать звонок</a>
									</div>
								</div>
							</div>

							<div class="toolbar-buttons">
								<div class="toolbar-buttons__favorite">
									<a href="favorite.html">
										<img src="img/icon-svg/fav.svg" width="22" height="24" alt="favorite" loading="lazy">
									</a>
								</div>
								<div class="toolbar-buttons__comparison">
									<a href="comparison.html">
										<svg width="14px" height="14px" class="icon-comparison">
											<use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="img/icon-svg/sprite/sprite.svg#comparison"></use>
										</svg>
									</a>
								</div>
								<div class="toolbar-buttons__basket">
									<div class="basket">
										<a href="order.html">
											<span class="basket__icon">
												<svg width="20px" height="19px" class="icon-cart">
													<use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="img/icon-svg/sprite/sprite.svg#cart"></use>
												</svg>

												<span class="basket__count">4</span>
											</span>
										</a>
									</div>
								</div>
								<div class="toolbar-buttons__auth">
									<a href="">
										<svg width="16px" height="19px" class="icon-user">
											<use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="img/icon-svg/sprite/sprite.svg#user"></use>
										</svg>
									</a>
								</div>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
	<div id="header-boundry" class="header-desk__bottom">
		<div class="uk-container">
			<div class="header-desk__inner">
				<div class="uk-grid">
					<div class="uk-width-auto">
						<div class="header-nav-buttons">
							<div class="header-nav-buttons__item">
								<a href="" class="btn btn--icon">
									<svg width="12px" height="12px" class="icon-catalog">
										<use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="img/icon-svg/sprite/sprite.svg#catalog"></use>
									</svg>
									<span>Каталог товаров</span>
								</a>
								<div class="dropdown-menu" uk-dropdown="pos: bottom-justify; offset: 0; boundary: #header-boundry; boundary-align: true; delay-hide: 100;">
									<div class="uk-container">
										<div class="dropdown-menu__inner">
											<div class="uk-grid">
												<div class="uk-width-1-2@m">
													<div class="title-h1">В нашем каталоге
														<br>
														более 30 производителей</div>

													<div class="brands-pictures">
														<ul class="brands-pictures__list">
															{% for i in 1..13 %}
																<li class="brands-pictures__list-item"><img src="img/media/webp/brand-img-{{i}}.webp" alt="" loading="lazy"></li>
															{% endfor %}
														</ul>
														<a href="" class="uk-visible@m">Все производители</a>
													</div>
												</div>
												<div class="uk-width-1-2@m">
													<ul class="brands-titles">
														{% for i in 1..3 %}
															<li>
																{% for k in 1..18 %}
																	<a href="">Brand-name-{{k}}</a>
																{% endfor %}
															</li>
														{% endfor %}
													</ul>
													<a href="" class="uk-hidden@m">Все производители</a>
												</div>
											</div>
										</div>
									</div>
								</div>
							</div>
							<div class="header-nav-buttons__item">
								<a href="" class="btn btn--white">Заявка на расчет</a>
							</div>
						</div>
					</div>
					<div class="uk-width-expand">
						<nav class="main-nav">
							<ul class="main-nav__list">
								<li class="main-nav__item">
									<a href="" class="main-nav__link">Бренды</a>
									<div class="dropdown-menu" uk-dropdown="pos: bottom-justify; offset: 0; boundary: #header-boundry; boundary-align: true; delay-hide: 100;">
										<div class="uk-container">
											<div class="dropdown-menu__inner">
												<div class="uk-grid" uk-grid>
													<div class="uk-width-1-3">
														<div class="dropdown-menu__item">
															<ul class="dropdown-menu__list">
																<li>
																	<a>Лицензии 1С:Предприятие 8</a>
																</li>
																<li>
																	<a>1С:Бухгалтерия 8</a>
																</li>
																<li>
																	<a>1С:Зарплата и Управление Персоналом 8</a>
																</li>
																<li>
																	<a>1С:Управление Торговлей 8</a>
																</li>
																<li>
																	<a>1С Комплексная Автоматизация 8</a>
																</li>
															</ul>
														</div>
													</div>
													<div class="uk-width-1-3">
														<div class="dropdown-menu__item">
															<ul class="dropdown-menu__list">
																<li>
																	<a>Лицензии 1С:Предприятие 8</a>
																</li>
																<li>
																	<a>1С:Бухгалтерия 8</a>
																</li>
																<li>
																	<a>1С:Зарплата и Управление Персоналом 8</a>
																</li>
																<li>
																	<a>1С:Управление Торговлей 8</a>
																</li>
																<li>
																	<a>1С Комплексная Автоматизация 8</a>
																</li>
															</ul>
														</div>
													</div>

													<div class="uk-width-1-3">
														<div class="dropdown-menu__item">
															<div class="title-h3">Перейди в облако.
																<br>Это надежнее, быстрее и дешевле</div>

															<div class="btn-wrapper">
																<a href="" class="btn btn--yellow">15 дней бесплатно</a>
															</div>
														</div>
													</div>
												</div>
											</div>
										</div>
									</div>
								</li>
								<li class="main-nav__item">
									<a href="" class="main-nav__link">О компании</a>
								</li>
								<li class="main-nav__item">
									<a href="" class="main-nav__link">Оплата и доставка</a>
								</li>
								<li class="main-nav__item">
									<a href="" class="main-nav__link">Партнерам</a>
								</li>
								<li class="main-nav__item">
									<a href="" class="main-nav__link main-nav__link--icon">
										<svg width="14px" height="14px" class="icon-percent">
											<use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="img/icon-svg/sprite/sprite.svg#percent"></use>
										</svg>
										<span>Купон на скидку</span>
									</a>
								</li>
								<li class="main-nav__item">
									<a href="" class="main-nav__link">Контакты</a>
								</li>
							</ul>
						</nav>
					</div>
				</div>
			</div>
		</div>
	</div>
</div>
