{% set menu = [
    'Личные данные',
    'Избранное',
    'Мои заказы',
    'Мои адреса'
  ] %}

<ul class="aside-list-nav">
	{% for item in menu %}
		{% if item == active %}
			<li>
				<a href="#" class="active">{{item}}</a>
			</li>
		{% else %}

			<li>
				<a href="#">{{item}}</a>
			</li>
		{% endif %}
	{% endfor %}
	<li>
		<a href="#">Выйти</a>
	</li>
</ul>
